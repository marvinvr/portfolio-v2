{
  "name": "portfolio-v2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "whoop:token": "node scripts/get-whoop-token.js",
    "whoop:clear": "node scripts/clear-whoop-stats.js",
    "postbuild": "bunx svelte-sitemap --out-dir ./build/prerendered --change-freq daily --domain https://marvinvr.ch && mv ./build/prerendered/sitemap.xml ./build/client/sitemap.xml"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.31",
    "sass": "^1.69.5",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-sitemap": "^2.6.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.4.1",
    "typescript": "^5.5.0",
    "vite": "^5.4.4",
    "vite-imagetools": "^6.2.3"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^8.13.0",
    "@sveltejs/adapter-node": "^5.2.5",
    "lucide-svelte": "^0.540.0",
    "mathlifier": "^1.3.4",
    "node-cache": "^5.1.2",
    "node-html-parser": "^6.1.11",
    "redis": "^5.8.2",
    "rehype-external-links": "^3.0.0",
    "shiki": "^0.14.5",
    "svelte-lazy-image": "^0.5.1",
    "svelte-material-icons": "^3.0.5"
  }
}